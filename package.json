{
  "name": "fixat",
  "version": "0.1.1",
  "description": "A library for test fixtures",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.ts",
        "default": "./dist/react.js"
      }
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Victorystick/fixat.git"
  },
  "author": "Oskar Segersv√§rd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Victorystick/fixat/issues"
  },
  "homepage": "https://github.com/Victorystick/fixat#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.9",
    "jest": "^30.0.5",
    "jsdom": "^26.1.0",
    "react": "^19.1.1",
    "tsdown": "^0.13.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@testing-library/react": "^16.3.0",
    "react": "^19.1.1"
  },
  "peerDependenciesMeta": {
    "@testing-library/react": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}
